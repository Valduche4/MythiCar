<div>
  <h1>Happy to see you again</h1>
  <h3>My cars</h3>
</div>

<div>
  <% @user.cars.each do |car| %>
    <%= link_to car.brand, car_path(car) %>
    <%= cl_image_tag car.photo.key, height: 75, width: 100, crop: :fill %>
  <% end %>
</div>

<div>
  <%= link_to "Add a car", new_car_path %>
</div>
